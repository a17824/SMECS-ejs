

<!-- Custom Theme Style -->
<link href="/public/gentelella/build/css/custom.css" rel="stylesheet">
<link href="/public/gentelella/build/css/showAvatar.css" rel="stylesheet">

<div class="navbar nav_title" style="border: 0;">
    <a href="/dashboard" class="site_title"><img src="/schoolLogos/STS-Logo-Horizontal.png"> </a>
</div>

<div class="clearfix"></div>

<div class="profile clearfix">
    <div class="profile_pic">
        <% if (user.photo == "" || user.photo == undefined) { %>
            <img src="/photosNotAvailable/photoNotAvailable.bmp" alt="..." class="img-circle profile_img">
        <% } else { %>
            <img src="/photosUsers/<%= userAuthPhoto %>" alt="..." class="avatarMenuProfile">
        <% } %>
    </div>
    <div class="profile_info">
      <span>Welcome,</span>
      <h2><%= userAuthName %></h2>
    </div>
  </div>