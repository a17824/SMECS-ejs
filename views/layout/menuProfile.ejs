<div class="clearfix"></div>

<div class="profile clearfix">

    <div class="profile_pic">
        <% if (user.photo == "" || user.photo == undefined) { %>
            <img src="/photosNotAvailable/user.png" alt="..." class="img-circle profile_img">
        <% } else { %>
            <img src="/photosUsers/<%= userAuthPhoto %>" alt="..." class="avatarMenuProfile">
        <% } %>
    </div>
    <div class="profile_info">
      <span>Welcome,</span>
      <h2><%= userAuthName %></h2>
    </div>
</div>
