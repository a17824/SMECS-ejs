    <script>

        //Request calling 911
        var choices;
        function selectUser(select)
        {
            var option = select.options[select.selectedIndex];
            var ul = select.parentNode.getElementsByTagName('ul')[0];
            choices = ul.getElementsByTagName('input');

            for (var i = 0; i < choices.length; i++) {
                if (choices[i].value == option.value){
                    return;
                }
            }

            var li = document.createElement('li');
            var input = document.createElement('input');
            var text = document.createTextNode(option.firstChild.data);

            input.type = 'hidden';
            input.name = 'users[]';
            input.value = option.value;

            li.appendChild(input);
            li.appendChild(text);
            li.setAttribute('onclick', 'this.parentNode.removeChild(this);');

            ul.appendChild(li);
        }
        var usersToRemove = [];
        function removeUser(user){
            var choice = user.getElementsByTagName('input');
            usersToRemove.push(choice[0].value);
        }
        //end of Request calling 911

        //DELETE Confirmation
        function YNconfirm(type, optionName) {
            if (confirm('ATTENTION: \'' + optionName + '\' option will be deleted. Are you sure?'))
                return true;
            else
                return false;
        };

        //if send email checkbox changes, hides/shows div
        function checkOnOff() {
            if (document.getElementById('checkboxSendEmail').checked) {
                $('#emailAddressLabel').fadeIn("medium"); //to remove display: none
                $('#emailAddressSelect').fadeIn("medium"); //to remove display: none
            }
            else {
                $('#emailAddressLabel').fadeOut("medium"); //to remove display: none
                $('#emailAddressSelect').fadeOut("medium"); //to remove display: none
            }
        }

        $(document).ready(function(){

            var oldSortID = $("#oldSortID").val();
            var select_sort = '';
            var sortArray = $("#inUseSortID").val().split(",").map(Number);
            for (var a=1;a<=99;a++){
                if(sortArray.indexOf(a) == -1 || a == oldSortID){
                    if(a == oldSortID){
                        select_sort += '<option val=' + a + ' selected="selected">' + a + '</option>';
                    }else{
                        select_sort += '<option val=' + a + '>' + a + '</option>';
                    }
                }
            }
            $('#sort_select').html(select_sort);

            var oldAlertID = $("#oldAlertID").val();
            var select = '';
            var myArray = $("#inUseAlertID").val().split(",").map(Number);
            for (var i=1;i<=99;i++){
                if(myArray.indexOf(i) == -1 || i == oldAlertID){
                    if(i == oldAlertID){
                        select += '<option val=' + i + ' selected="selected">' + i + '</option>';
                    }else{
                        select += '<option val=' + i + '>' + i + '</option>';
                    }
                }
            }
            $('#some_select').html(select);



            function radioOff() {
                $('#smecsUserSelect').removeClass().addClass('disable');
                $("#smecsUserSelect").attr('disabled',true);
                $('#imgAdd').removeClass().addClass('disable');
                $('#smecsUserLabel').removeClass().addClass('disable');
                $('#smecsUserDiv').removeClass().addClass('disable');
                $('#smecsUserUl').removeClass().addClass('disable');
                $('#clickHere').removeClass().addClass('disable');
            }
            function radioOn() {
                $('#smecsUserSelect').removeClass().addClass('enable');
                $("#smecsUserSelect").attr('disabled',false);
                $('#imgAdd').removeClass().addClass('enable');
                $('#smecsUserLabel').removeClass().addClass('enable');
                $('#smecsUserDiv').removeClass().addClass('enable');
                $('#smecsUserUl').removeClass().addClass('enable');
                $('#clickHere').removeClass().addClass('enable');
            }


            function radioOffAutoDrill() {
                $('#autoDrillDiv').removeClass().addClass('disableAutoDrill');
            }
            function radioOnAutoDrill() {
                $('#autoDrillDiv').removeClass().addClass('enableAutoDrill');
            }


            //This will check the status of radio button on page load
            $('input[name=request911]:checked').each(function() {
                var checkVal = $(this).val();
                if (checkVal == 'false'){
                    radioOff();
                } else {
                    $('#smecsUserDiv').show(); //to remove display: none
                    radioOn();
                }
            });

            $('input[name=autoDrill]:checked').each(function() {
                var checkVal = $(this).val();
                if (checkVal == 'false'){
                    radioOffAutoDrill();
                } else {
                    $('#autoDrillDiv').show(); //to remove display: none
                    radioOnAutoDrill();
                }
            });

            //This will check the status of radio button onclick
            $('#request911').on('ifChecked', function () {
                radioOff();
            })
            $('#request911').on('ifUnchecked', function () {
                $('#smecsUserDiv').show(); //to remove display: none
                radioOn();
            })


            $('#autoDrill').on('ifChecked', function () {
                radioOffAutoDrill();
            })
            $('#autoDrill').on('ifUnchecked', function () {
                $('#autoDrillDiv').show(); //to remove display: none
                radioOnAutoDrill();
            })


            //This will check the status of 'Send email'checkbox button on page load
            $('input[name=checkboxSendEmail]:checked').each(function() {
                var checkVal = $(this).val();
                if (checkVal == 'false'){
                    $('#emailAddressLabel').hide(); //to remove display: none
                    $('#emailAddressSelect').hide(); //to remove display: none
                } else {
                    $('#emailAddressLabel').show(); //to remove display: none
                    $('#emailAddressSelect').show(); //to remove display: none
                }
            });


            var id,alertName,alertID,alertProcedure,request911Call,autoDrill,sortID, icon,alertLight,alertLightSound;
            $("#<%= alert.id %>").click(function(){

                var alertGroupIdAndSound=$("#alertGroupID").val();
                var redirectAPI=$("#redirectAPI").val();
                if(alertGroupIdAndSound){
                    var alertGroupIdAndSoundArray = alertGroupIdAndSound.split(",").map(String);
                    var alertGroupID = parseInt(alertGroupIdAndSoundArray[0]);
                    var alertGroupName = alertGroupIdAndSoundArray[1];
                    var sound = alertGroupIdAndSoundArray[2];
                }

                var emailIdAndName=$("#emailID").val();
                if(emailIdAndName){
                    var emailIdAndNameArray = emailIdAndName.split(",").map(String);
                    var emailID = parseInt(emailIdAndNameArray[0]);
                    var email = emailIdAndNameArray[1];
                }

                id=$(this).attr('id');
                request911Call=$("input:radio[name='request911']:checked").val();
                autoDrill=$("input:radio[name='autoDrill']:checked").val();
                alertLight=$("input:radio[name='alertLight']:checked").val();
                alertLightSound=$("input:radio[name='alertLightSound']:checked").val();
                alertName=$("#alertName").val();
                alertID=$("#some_select").val();
                alertProcedure=$("#alertProcedure").val();
                sortID=$("#sort_select").val();
                icon=$("#selected-text").val();
                var alertRequestProcedureCompleted = document.getElementById("checkboxProc").checked;
                var alertRequestWeAreSafe = document.getElementById("checkboxSafe").checked;
                var alertRequestForINeedHelp = document.getElementById("checkboxNeedHelp").checked;
                var alertRequestSendEmail = document.getElementById("checkboxSendEmail").checked;
                var alertRequestSendText = document.getElementById("checkboxSendText").checked;

                var occurrences = $("#occurrences").val();
                var fullStartDate = $("#startDate").val();
                var fullEndDate = $("#endDate").val();
                var autoDrillNumber = $("#autoDrillNumber").val();
                var e = document.getElementById("everyType");
                var everyType = e.options[e.selectedIndex].value;
                var sunday = document.getElementById("sunday").checked;
                var monday = document.getElementById("monday").checked;
                var tuesday = document.getElementById("tuesday").checked;
                var wednesday = document.getElementById("wednesday").checked;
                var thursday = document.getElementById("thursday").checked;
                var friday = document.getElementById("friday").checked;
                var saturday = document.getElementById("saturday").checked;

                var fullTime = $("#myDatepicker3").find("input").val();


                var arrayUsers = [];
                if (choices === undefined){
                    arrayUsers = $("#savedValuewhoCanCall911").val().split(",");
                    for (var a = 0; a < usersToRemove.length; a++) {
                        for (var i = 0; i < arrayUsers.length; i++) {
                            if (usersToRemove[a] == arrayUsers[i]){
                                arrayUsers.splice(i, 1);
                            }
                        }
                    }

                } else {
                    if(choices.length > 0) {
                        for (var a = 0; a < choices.length; a++) {
                            arrayUsers.push(choices[a].value);
                        }
                    }
                }

                //-----forms validation------------------------------------
                if (alertName == "" ) {
                    alert("Alert NAME must be filled out");
                    return false;
                }

                if (alertGroupID == null ) {
                    alert("Alert Group must be filled out");
                    return false;
                }
                /*strange behaviour when saving and radio smecs == true and no user is in arrayUsers.
                 Its s adding 1 user in arrayUsers(arrayUsers[0] == "").
                 Following "if" is to delete this user */
                if(arrayUsers.length == 1 && arrayUsers[0] == ""){
                    let index = arrayUsers.indexOf(5);
                    arrayUsers.splice( index, 1 );
                }

                if(request911Call == 'true' && arrayUsers.length < 1 ){
                    alert("You have selected that this alert request a 911 call. Please select at least one user role/group from dropdown list");
                    return false;
                }
                /*if(autoDrillNumber > 35) {
                    alert("REPEAT EVERY: needs to be a number between 1 and 35");
                    return false;
                }*/

                if(autoDrill === 'true') {
                    var validStartDate = moment(fullStartDate, 'MM/DD/YYYY',true).isValid()
                    var validEndDate = moment(fullEndDate, 'MM/DD/YYYY',true).isValid()
                    if(!validStartDate) {
                        alert("Start date is incorrect. Please input MM/DD/YYYY");
                        return false;
                    }
                    if(!validEndDate) {
                        alert("End date is incorrect. Please input MM/DD/YYYY");
                        return false;
                    }
                    if(fullStartDate >= fullEndDate) {
                        alert("Date range incorrect. End Date needs to be earlier than Start date");
                        return false;
                    }

                    if(!sunday && !monday && !tuesday && !wednesday && !thursday && !friday && !saturday) {
                        alert("REPEAT ON incorrect. Choose at least one day");
                        return false;
                    }
                }


                //--------------------------------forms validation end-------

                $.post("/alerts/updateAlerts",{
                    alertToUpdate: id,
                    alertGroupName: alertGroupName,
                    request911Call: request911Call,
                    autoDrill: autoDrill,
                    whoCanCall911: arrayUsers,
                    alertGroupID: alertGroupID,
                    alertName: alertName,
                    alertID: alertID,
                    alertProcedure: alertProcedure,
                    oldAlertID: oldAlertID, //to update  aclPermission
                    sortID: sortID,
                    alertRequestProcedureCompleted: alertRequestProcedureCompleted,
                    alertRequestWeAreSafe: alertRequestWeAreSafe,
                    alertRequestForINeedHelp: alertRequestForINeedHelp,
                    alertRequestSendEmail: alertRequestSendEmail,
                    alertRequestSendSMS: alertRequestSendText,
                    alertLight: alertLight,
                    alertLightSound: alertLightSound,
                    icon: icon,
                    sound: sound,
                    emailID: emailID,
                    occurrences: occurrences,
                    fullStartDate: fullStartDate,
                    fullEndDate: fullEndDate,
                    autoDrillNumber: autoDrillNumber,
                    everyType: everyType,
                    sunday: sunday,
                    monday: monday,
                    tuesday: tuesday,
                    wednesday: wednesday,
                    thursday: thursday,
                    friday: friday,
                    saturday: saturday,
                    fullTime: fullTime

                },function(data){
                    if(data==='done')
                    {
                        alert("login success");
                    }
                    window.location = data.redirect
                }).error(function(error){
                    if(error.responseText == 'showAlert')
                        alert("Alert Name already already in use. Choose a different Name.")

                });

            });
        });
    </script>


</head>

