    <script>

        var choices;
        function selectUser(select)
        {
            var option = $("#dataList").val();
            //get datalist data-value to value2send
            var value2send= document.querySelector('#students option[value="'+option+'"]').dataset.value;

            var ul = select.parentNode.getElementsByTagName('ul')[0];
            choices = ul.getElementsByTagName('input');

            for (var i = 0; i < choices.length; i++) {

                if (choices[i].value == value2send) {
                    return;
                }
            }
            var li = document.createElement('li');
            var input = document.createElement('input');
            var text = document.createTextNode(option);

            input.type = 'hidden';
            input.name = 'users[]';
            input.value = value2send;

            li.appendChild(input);
            li.appendChild(text);
            li.setAttribute('onclick', 'this.parentNode.removeChild(this);');

            ul.appendChild(li);


        }
        var usersToRemove = [];
        function removeUser(user){
            var choice = user.getElementsByTagName('input');
            usersToRemove.push(choice[0].value);
        }



        $(document).ready(function(){
            var newPhoto;
            var pathPhoto;


            function checkBoxOffOther() {
                $('#other').removeClass().addClass('disable');
                $('#firstName').prop( "disabled", true ); //Disable
                $('#lastName').prop( "disabled", true ); //Disable
                $('#labelsDiv1').removeClass().addClass('labelsDivDisable');
            }
            function checkBoxOnOther() {
                $('#other').removeClass().addClass('enable');
                $('#firstName').prop( "disabled", false ); //Enable
                $('#lastName').prop( "disabled", false ); //Enable
                $('#labelsDiv1').removeClass().addClass('labelsDivEnable');
            }

            //This will check the status of radio button on page load/page refresh
            $('input[class=other]').each(function() {
                var checkVal = $('.other:checked');
                if (checkVal.length < 1){
                    checkBoxOffOther();
                } else {
                    checkBoxOnOther();
                }
            });
            //This will check the status of radio button onclick
            $('input:checkbox[class=other]').change(function() {
                var checkVal = $('.other:checked');
                if (checkVal.length < 1){
                    checkBoxOffOther();
                } else {
                    checkBoxOnOther();
                }
            });

            function checkBoxOffParents() {
                $('#dataList').removeClass().addClass('disable');
                $("#dataList").attr('disabled',true);
                $('#imgAdd').removeClass().addClass('disable');
                $('#smecsUserLabel').removeClass().addClass('disable');
                $('#smecsUserDiv').removeClass().addClass('disable');
                $('#smecsUserUl').removeClass().addClass('disable');
                $('#clickHere').removeClass().addClass('disable');
            }
            function checkBoxOnParents() {
                $('#dataList').removeClass().addClass('enable');
                $("#dataList").attr('disabled',false);
                $('#imgAdd').removeClass().addClass('enable');
                $('#smecsUserLabel').removeClass().addClass('enable');
                $('#smecsUserDiv').removeClass().addClass('enable');
                $('#smecsUserUl').removeClass().addClass('enable');
                $('#clickHere').removeClass().addClass('enable');
            }

            //This will check the status of radio button on page load/page refresh
            $('input[id=parents]').each(function() {
                var checkVal = $(this).is(':checked');
                if (checkVal == false){
                    checkBoxOffParents();
                } else {
                    checkBoxOnParents();
                }
            });
            //This will check the status of radio button onclick
            $('input:checkbox[id=parents]').change(function() {
                var checkVal = $(this).is(':checked');
                if (checkVal == false){
                    checkBoxOffParents();
                } else {
                    checkBoxOnParents();
                }
            });

            function checkBoxOffUtilityName() {
                $('#companyName').removeClass().addClass('disable');
                $('#companyNameText').removeClass().addClass('disable');
                $('#contactName').removeClass().addClass('disable');
                $('#contactNameText').removeClass().addClass('disable');
                $('#labelsDiv3').removeClass().addClass('labelsDivDisable');
            }
            function checkBoxOnUtilityName() {
                $('#companyName').removeClass().addClass('enable');
                $('#companyNameText').removeClass().addClass('enable');
                $('#companyName').removeClass().addClass('enable');
                $('#contactNameText').removeClass().addClass('enable');
                $('#labelsDiv3').removeClass().addClass('labelsDivEnable');
            }

            //This will check the status of radio button on page load/page refresh
            $('input[class=utilityCompany]').each(function() {
                var checkVal = $(this).is(':checked');
                if (checkVal == false){
                    checkBoxOffUtilityName();
                } else {
                    checkBoxOnUtilityName();
                }
            });
            //This will check the status of radio button onclick
            $('input:checkbox[class=utilityCompany]').change(function() {
                var checkVal = $(this).is(':checked');
                if (checkVal == false){
                    checkBoxOffUtilityName();
                } else {
                    checkBoxOnUtilityName();
                }
            });




            var userID,userPrivilegeName,userPrivilegeID,firstName,lastName,email,newPin, oldPin,confirmPin,oldPhoto,companyNameText,contactNameText;
            $("#<%= users.id %>").click(function(){
                userID=$(this).attr('id');

                var checkedRoleID = [];
                $.each($("input[name='checkbox']:checked"), function(){
                    checkedRoleID.push($(this).val());
                });
                var checkedRoleName;
                checkedRoleName = $("input[name='checkbox']:checked").next('label').map(function(){
                    return $(this).text();
                }).get();

                userPrivilegeID=$("input:radio[name='userPrivilegeID']:checked").val();
                userPrivilegeName = $("input:radio[name='userPrivilegeID']:checked + label").text();
                firstName=$("#firstName").val();
                lastName=$("#lastName").val();
                email=$("#email").val();
                oldPin=$("#oldPin").val();
                newPin=$("#newPin").val();
                confirmPin=$("#confirmPin").val();
                oldPhoto=$("#oldPhoto").val();
                companyNameText=$("#companyNameText").val();
                contactNameText=$("#contactNameText").val();

                var arrayUsers = [];
                if (choices === undefined){
                    arrayUsers = $("#savedValueSmecsUsers").val().split(",");
                    for (var a = 0; a < usersToRemove.length; a++) {
                        for (var i = 0; i < arrayUsers.length; i++) {
                            if (usersToRemove[a] == arrayUsers[i]){
                                arrayUsers.splice(i, 1);
                            }
                        }
                    }
                } else {
                    if(choices.length > 0) {
                        for (var a = 0; a < choices.length; a++) {
                            arrayUsers.push(choices[a].value);
                        }
                    }
                }

                if(arrayUsers.length == 1 && arrayUsers[0] == "") {
                    var index = arrayUsers.indexOf(5);
                    arrayUsers.splice(index, 1);
                }



                //-----forms validation------------------------------------

                var mailFormat = /^([a-z A-Z 0-9 _\.\-])+\@(([a-z A-Z 0-9\-])+\.)+([a-z A-z 0-9]{2,4})+$/;
                if (!mailFormat.test(email)) {
                    alert("Please input a valid EMAIL address!");
                    return false;
                }
                if (newPin != confirmPin) {
                    alert("Pin and Confirm Pin don't match!");
                    return false;
                }
                if (newPin !== oldPin) {
                    var pinFormat = /^\d{4}$/;
                    if (!pinFormat.test(newPin)) {
                        alert("Pin code should be 4 digits ");
                        return false;
                    }
                    if (!pinFormat.test(confirmPin)) {
                        alert("Confirm Pin code should be 4 digits ");
                        return false;
                    }
                    if (newPin != confirmPin) {
                        alert("Pin and Confirm Pin don't match!");
                        return false;
                    }
                }
                if (checkedRoleID.length < 1) {
                    alert("You must choose at least one USER ROLE");
                    return false;
                }else{
                    for (var a = 0; a < checkedRoleID.length; a++) {
                        if (checkedRoleID[a] != 99) {
                            if (firstName == "" ) {
                                alert("FIRST NAME must be filled out");
                                return false;
                            }
                            if (lastName == "" ) {
                                alert("LAST NAME must be filled out");
                                return false;
                            }
                        }
                        // Ask user if he wants the program to change automatically user privilege to regular user
                        /*else {
                            if (userPrivilegeID != 5 && confirm('Since user role has only "Parent" or "Utility Company" selected, user privilege will change to "Regular User". Do you want to continue?')) {
                                $("#5").prop("checked", true);
                                userPrivilegeID=$("input:radio[name='userPrivilegeID']:checked").val();
                                userPrivilegeName = $("input:radio[name='userPrivilegeID']:checked + label").text();
                                return false;
                            } else {
                                // Do nothing!

                            }
                        }*/
                        if (checkedRoleID[a] == 98 ) {
                            if (arrayUsers.length < 1) {
                                alert("Parent Role is checked. Please choose a student that belong to this parent or uncheck Parent Role from this user.");
                                return false;
                            }
                        }
                        if (checkedRoleID[a] == 99) {
                            if (companyNameText == "") {
                                alert("COMPANY NAME must be filled out");
                                return false;
                            }
                        }
                        if (checkedRoleID[a] == 99 && checkedRoleID.length == 1) {
                            if (contactNameText == "") {
                                alert("CONCTAC NAME must be filled out");
                                return false;
                            }
                        }
                    }
                }

                if (userPrivilegeID == null ) {
                    alert("USER PRIVILEGE must be filled out");
                    return false;
                }

                //--------------------------------forms validation end-------
                var pin;
                if (newPin !== oldPin) {
                    pin = newPin;

                } else{
                    pin = "oldPin";
                }
                $.post("/users/updateUser", {
                    userToUpdate: userID,
                    userRoleID: checkedRoleID,
                    userRoleName: checkedRoleName,
                    userPrivilegeID: userPrivilegeID,
                    userPrivilegeName: userPrivilegeName,
                    firstName: firstName,
                    lastName: lastName,
                    email: email,
                    pin: pin,
                    parentOf: arrayUsers,
                    companyName: companyNameText,
                    contactName: contactNameText,
                    pathPhoto: pathPhoto,
                    oldPhoto: oldPhoto,
                    newPhoto: newPhoto

                },function(data){
                    if(data==='done')
                    {
                        alert("login success");
                    }
                    window.location = data.redirect
                }).error(function (error) {
                    if (error.responseText == 'showAlert')
                        alert("Email already in use. Choose a different Email.")
                });

            });
        });
    </script>


</head>

